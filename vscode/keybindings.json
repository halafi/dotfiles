[
  {
    "key": "cmd+e n",
    "command": "explorer.newFile"
  },
  {
    "key": "cmd+e f",
    "command": "explorer.newFolder"
  },
  {
    "key": "cmd+e r",
    "command": "workbench.files.action.refreshFilesExplorer"
  },
  {
    "key": "alt+cmd+w",
    "command": "projectManager.listProjectsNewWindow"
  },
  {
    "key": "ctrl+n", 
    "command": "workbench.action.navigateLeft", 
    // "when": "editorTextFocus && vim.active && vim.mode != 'Insert'"
  },
  // {
  //   "key": "ctrl+h", 
  //   "command": "workbench.action.focusSideBar", 
  //   "when": "editorTextFocus && vim.active && vim.mode != 'Insert' && sideBarFocus"
  // },
  {
    "key": "ctrl+o", 
    "command": "workbench.action.navigateRight", 
    "when": "!panelFocus"
  },
  {
    "key": "ctrl+i",
    "command": "workbench.action.navigateUp",
  },
  {
    "key": "ctrl+enter",
    "command": "workbench.action.toggleMaximizedPanel",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+e",
    "command": "workbench.action.navigateDown",
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  // {
  //   "key": "ctrl+h",
  //   "command": "workbench.action.previousPanelView",
  //   "when": "panelFocus"
  // },
  // {
  //   "key": "ctrl+l",
  //   "command": "workbench.action.nextPanelView",
  //   "when": "panelFocus"
  // },
  // {
  //   "key": "cmd+shift+[",
  //   "command": "workbench.panel.output.focus",
  //   "when": "workbench"
  // },
  {
    "key": "ctrl+k",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+l", 
    "command": "workbench.action.focusActiveEditorGroup", 
    "when": "!editorTextFocus && !panelFocus"
  },
  {
    "key": "ctrl+enter",
    "command": "problems.action.openToSide",
    "when": "problemFocus"
  },
  {
    "key": "ctrl+enter",
    "command": "-problems.action.openToSide",
    "when": "problemFocus"
  },
  {
    "key": "ctrl+enter",
    "command": "search.action.openResultToSide",
    "when": "fileMatchOrMatchFocus && searchViewletVisible"
  },
  {
    "key": "ctrl+enter",
    "command": "-search.action.openResultToSide",
    "when": "fileMatchOrMatchFocus && searchViewletVisible"
  },
  {
    "key": "cmd+1",
    "command": "workbench.action.firstEditorInGroup"
  },
  {
    "key": "ctrl+e",
    "command": "search.action.focusSearchList",
    "when": "hasSearchResult && searchInputBoxFocus"
  },
  {
    "key": "ctrl+i",
    "command": "search.action.focusSearchFromResults",
    "when": "hasSearchResult && searchViewletFocus"
  },
  {
    "key": "cmd+up",
    "command": "-search.action.focusSearchFromResults",
    "when": "accessibilityModeEnabled && searchViewletVisible || firstMatchFocus && searchViewletVisible"
  },
  {
    "key": "ctrl+f e",
    "command": "search.action.expandSearchResults",
    "when": "hasSearchResult"
  },
  {
    "key": "ctrl+f r",
    "command": "search.action.collapseSearchResults",
    "when": "hasSearchResult"
  },
  {
    "key": "alt+w",
    "command": "git.openFile"
  },
  {
    "key": "cmd+0",
    "command": "-workbench.action.focusSideBar"
  },
  {
    "key": "cmd+0",
    "command": "workbench.action.zoomReset"
  },
  {
    "key": "cmd+numpad0",
    "command": "-workbench.action.zoomReset"
  }
]