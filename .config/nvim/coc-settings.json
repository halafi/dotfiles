{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "coc.preferences.diagnostic.virtualText": true,
  "coc.preferences.useQuickfixForLocations": false,
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "eslint.run": "onSave",
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown"],
  "suggest.noselect": false
}
